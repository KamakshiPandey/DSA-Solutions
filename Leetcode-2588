class Solution {
public:
    long long beautifulSubarrays(vector<int>& nums) {
        int n=nums.size();
        unordered_map<int,int>m;
        m[0]=1;
        int prexor=0;
        long long ans=0;
        for(int i=0;i<n;i++){
            prexor^=nums[i];
            if(m.find(prexor)!=m.end()){
                ans+=m[prexor];
            }
            m[prexor]++;


        }
        return ans;
        
    }
};
