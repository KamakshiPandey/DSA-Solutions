class Solution {
public:
int unique(vector<int>&v,int n){
    int l=0,r=n-1;
    while(l<r){
        int mid=l+(r-l)/2;
        if(mid%2==1){
            mid--;
        }
        if(v[mid]==v[mid+1]){
            l=mid+2;

        }
        else{
            r=mid;
        }
    }
    return v[r];

}
    int singleNonDuplicate(vector<int>& nums) {
        int n=nums.size();
        int m=unique(nums,n);
        return m;
        
    }
};
